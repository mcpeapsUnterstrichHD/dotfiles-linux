{
	"$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
	"logo": {
		/*"type": "auto",
		"source": "/Users/mahd/DEV/dotfiles/.config/fastfetch/logo.png",*/
		"width": 40,
		"height": 22,
		"padding": {
			"top": 5,
			"left": 3,
			"right": 3
		},
		"position": "left",
		"printRemaining": true,
		"preserveAspectRatio": true,
		"chafa": {
			"canvasMode": "TRUECOLOR",
			"colorSpace": "RGB"
		},
		"recache": true
	},
	"general": {
		"multithreading": true,
		"processingTimeout": -1
	},
	"display": {
		"stat": false,
		"pipe": false,
		"showErrors": true,
		"disableLinewrap": true,
		"hideCursor": false,
		"separator": ": ",
		"brightColor": false,
		"key":{
			"width": 0
		},
		"size": {
		"binaryPrefix": "jedec",

			"ndigits": 3
		},
		"temp": {
			"units": "C",
			"ndigits": 3
		},
		"bar": {
			"charElapsed": "■",
			"charTotal": "-",
			"border": true
		},
		"percent": {
			"type": 2,
			"ndigits": 3
		}
	},
